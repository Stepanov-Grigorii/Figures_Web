<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>FillListServlet</servlet-name>
        <servlet-class>ru.figures.servlet.FillListServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>PrintListServlet</servlet-name>
        <servlet-class>ru.figures.servlet.PrintListServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>PrintSortedListServlet</servlet-name>
        <servlet-class>ru.figures.servlet.PrintSortedListServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>PrintRTCServlet</servlet-name>
        <servlet-class>ru.figures.servlet.PrintRTCServlet</servlet-class>
    </servlet>



    <servlet-mapping>
        <servlet-name>FillListServlet</servlet-name>
        <url-pattern>/fill</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>PrintListServlet</servlet-name>
        <url-pattern>/print</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>PrintSortedListServlet</servlet-name>
        <url-pattern>/prints</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>PrintRTCServlet</servlet-name>
        <url-pattern>/printRTC</url-pattern>
    </servlet-mapping>

</web-app>